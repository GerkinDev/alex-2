{% extends 'base.html.twig' %}

{% block body %}
<div class="templatemo-content-wrapper">
  <div class="templatemo-content">
    <section id="menu-section" class="active">
      <div class="row">
        <div class="col-sm-6 col-md-3 col-lg-3 col-xl-3 margin-bottom-20">
          <a href="/products" class="change-section">
            <div class="black-bg btn-menu">
              <i class="fa fa-cubes"></i>
              <h2>Products</h2>
            </div>
          </a>
        </div>
        <div class="col-sm-6 col-md-3 col-lg-3 col-xl-3 margin-bottom-20">
          <a href="/services" class="change-section">
            <div class="black-bg btn-menu">
              <i class="fa fa-laptop"></i>
              <h2>Services</h2>
            </div>
          </a>
        </div>
        <div class="col-sm-6 col-md-3 col-lg-3 col-xl-3 margin-bottom-20">
          <a href="/about" class="change-section">
            <div class="black-bg btn-menu">
              <i class="fa fa-users"></i>
              <h2>About</h2>
            </div>
          </a>
        </div>
        <div class="col-sm-6 col-md-3 col-lg-3 col-xl-3 margin-bottom-20">
          <a href="/contact" class="change-section">
            <div class="black-bg btn-menu">
              <i class="fa fa-envelope"></i>
              <h2>Contact</h2>
            </div>
          </a>
        </div>

        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 margin-bottom-20">
          <a href="/company-intro" class="change-section">
            <div class="black-bg btn-menu">
              <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
              <h2>Print your own model</h2>
            </div>
          </a>
        </div>
      </div>
    </section><!-- /.menu-section -->
  </div><!-- /.templatemo-content -->
</div><!-- /.templatemo-content-wrapper -->
</div><!-- /.row -->
{% endblock %}

{% block background_image %}zoom-bg-1.jpg{% endblock %}
